{
    "name": "template",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "lint": "eslint . --fix --cache",
        "format": "prettier --write .",
        "dev": "next dev --turbopack",
        "build": "next build --turbopack",
        "build-analyze": "ANALYZE=true next build --turbopack",
        "email:dev": "email dev -d ./components/templates/emails",
        "email:build": "email build -d ./components/templates/emails",
        "cf:typegen": "wrangler types -e local --env-interface CloudflareEnv ./cloudflare-env.d.ts",
        "cf:build:local": "opennextjs-cloudflare build -e local",
        "cf:build:production": "opennextjs-cloudflare build -e production",
        "cf:deploy:local": "opennextjs-cloudflare deploy -e local",
        "cf:deploy:production": "opennextjs-cloudflare deploy -e production",
        "db:generate": "drizzle-kit generate --config ./integrations/db/drizzle.config.ts",
        "db:migrate:local": "wrangler -e local d1 migrations apply template",
        "db:migrate:production": "wrangler -e production d1 migrations apply template",
        "db:studio:local": "NODE_ENV=development drizzle-kit studio --config ./integrations/db/drizzle.config.ts",
        "db:studio:production": "NODE_ENV=production drizzle-kit studio --config ./integrations/db/drizzle.config.ts"
    },
